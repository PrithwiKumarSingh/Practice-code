function greet(obj){
    console.log(`Hello ${obj.name} `);
}

function meet(obj){
    console.log(`Hello ${obj.name}, I will meet you in Delhi`);
}

function Edit(Name){
    console.log(`Edit ${Name}, of the user`);
}


