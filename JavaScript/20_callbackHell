function fetchdata(callback){
    console.log("Fetched info of user");

        setTimeout(()=>{
            console.log("User detail fetched succesfully");
            const obj = {
                name : "Rohit",
                age : 28,
                city : "Delhi",
            }

            callback(obj);
        },2000);

}

fetchdata(meet);